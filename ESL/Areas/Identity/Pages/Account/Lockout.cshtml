@page
@model LockoutModel
@{
    Layout = "/Views/Shared/_Login.cshtml";
    ViewData["Title"] = "Locked out";
}

<header style="margin: 0; text-align: center;">
    <h1 class="text-danger"><i class="fa fa-lock"></i>  @ViewData["Title"]</h1>
    <p class="text-danger">This account has been locked out, </br>please try again later or contact with administrator.</p>
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link" asp-area="Identity" asp-page="/Account/Login">Back</a>
        </li>
    </ul>
</header>
